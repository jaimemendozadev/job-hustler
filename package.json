  "scripts": {
    "test": "jest",
    "sls:deploy": "sls deploy --aws-profile jobhustler",
    "dev:start": "sls offline start",
    "sls:remove": "sls remove --aws-profile jobhustler",
    "fe:devstart": "webpack-dev-server --config ./webpack.config.js --mode development",
