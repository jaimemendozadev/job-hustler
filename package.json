    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "react-hot-loader": "^4.3.12",
    "react-testing-library": "^5.3.1",
    "regenerator-runtime": "^0.13.1",
    "serverless-offline": "^3.31.3",
