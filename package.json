  "scripts": {
    "test": "jest",
    "flow": "flow",
    "sls:deploy": "sls deploy --aws-profile jobhustler",
    "dev:start": "sls offline start",
    "sls:remove": "sls remove --aws-profile jobhustler",
    "fe:devstart": "webpack-dev-server --config ./webpack.config.js --env.mode development --env.presets ",
    "fe:prodstart": "webpack-dev-server --config ./webpack.config.js --env.mode production --env.presets ",
